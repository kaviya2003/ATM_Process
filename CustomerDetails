import java.util.*;
public class CustomerDetails {
	int accNo,pin,acbalancde;
	String name;
	public CustomerDetails(int accNo, String name, int pin, int acbalancde) {
		super();
		this.accNo = accNo;
		this.name = name;
		this.pin = pin;
		this.acbalancde = acbalancde;
	}
	public int getAccNo() {
		return accNo;
	}
	public void setAccNo(int accNo) {
		this.accNo = accNo;
	}
	public int getPin() {
		return pin;
	}
	public void setPin(int pin) {
		this.pin = pin;
	}
	public int getAcbalancde() {
		return acbalancde;
	}
	public void setAcbalancde(int acbalancde) {
		this.acbalancde = acbalancde;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
}
